{
  "name": "@ebizcard/web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "export": "next export",
    "build": "next build",
    "build:static": "cross-env NEXT_STATIC_EXPORT=true next build",
    "build:static:unix": "cross-env NEXT_STATIC_EXPORT=true next build",
    "build:s3": "npm run build:static",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "clean": "rm -rf .next out",
    "analyze": "ANALYZE=true next build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "setup-owner": "node ../../scripts/setup-first-owner.js",
    "setup-owner-simple": "node ../../scripts/setup-first-owner-simple.js",
    "setup-owner-interactive": "node ../../scripts/setup-owner-interactive.js",
    "setup-database": "node ../../scripts/setup-database.js",
    "setup-database-interactive": "node ../../scripts/setup-database-interactive.js",
          "deploy:functions": "powershell -ExecutionPolicy Bypass -File ./scripts/deploy-edge-functions.ps1",
          "deploy:function": "powershell -ExecutionPolicy Bypass -File ./scripts/deploy-edge-functions.ps1 -FunctionName",
          "check:functions": "powershell -ExecutionPolicy Bypass -File ./scripts/check-edge-functions.ps1",
          "fix:update-profile": "powershell -ExecutionPolicy Bypass -File ./scripts/fix-update-profile.ps1",
          "logs:function": "powershell -ExecutionPolicy Bypass -File ./scripts/view-edge-function-logs.ps1 -FunctionName"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^7.0.2",
    "@dnd-kit/utilities": "^3.2.2",
    "@ebizcard/shared-utils": "file:../../packages/shared-utils",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@react-email/render": "^2.0.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.38.0",
    "@tanstack/react-query": "^4.32.6",
    "autoprefixer": "^10.4.15",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "dompurify": "^3.0.6",
    "file-saver": "^2.0.5",
    "framer-motion": "^10.16.4",
    "isomorphic-dompurify": "^2.9.0",
    "lru-cache": "^10.1.0",
    "lucide-react": "^0.292.0",
    "next": "^13.5.4",
    "postcss": "^8.4.29",
    "qr-code-styling": "^1.9.2",
    "qrcode": "^1.5.3",
    "qrcode.react": "^3.2.0",
    "quill": "^2.0.3",
    "react": "18.2.0",
    "react-colorful": "^5.6.1",
    "react-dom": "18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.5.0",
    "react-qr-code": "^2.0.12",
    "react-quill": "^2.0.0",
    "resend": "^6.4.2",
    "tailwindcss": "^3.3.2",
    "vcard-json": "^0.1.0",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/dompurify": "^3.0.5",
    "@types/file-saver": "^2.0.5",
    "@types/node": "^20.0.0",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "cypress": "^13.3.0",
    "eslint": "^8.44.0",
    "eslint-config-next": "13.5.4",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "typescript": "^5.0.0",
    "cross-env": "^7.0.3"
  }
}
